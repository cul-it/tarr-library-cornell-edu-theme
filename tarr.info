name = Tarr
description = A Bootstrap Sub-theme.
core = 7.x
base theme = bootstrap

; Global regions
regions[navigation]     	= 'Navigation'
regions[highlighted]    	= 'Highlighted'
regions[help]           	= 'Help'
regions[content]        	= 'Content'
regions[footer_primary] 	= 'Footer primary'
regions[footer_secondary] 	= 'Footer secondary'

; Stylesheets
; style.css is referenced in html.tpl.php because when it's loaded via @import statement in Drupal, respond.js doesn't work so IE8 users only see the mobile layout
; see this jira issue for more info: https://issues.library.cornell.edu/browse/CULWEBDEVTEAM-431
; stylesheets[all][] = css/style.css

; Scripts
; scripts[] = js/bootstrap.min.js